{
    "name": "nyariv/shell-websocket",
    "description": "A php daemon that runs shell commands channeled through a websocket. Based on ReactPHP and Ratchet.",
    "keywords": ["ReactPHP", "Ratchet", "websocket", "shell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nir Yariv",
            "email": "nir@dofinity.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/nyariv/Ratchet"
        }
    ],
    "require": {
        "nyariv/wapi": "dev-master",
        "react/child-process": "^0.4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {"Wapi\\Daemon\\ShellWebsocket\\": "src/ShellWebsocket"}
    }
}
